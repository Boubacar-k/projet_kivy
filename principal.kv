#:import C kivy.utils.get_color_from_hex
#:import DropDown kivy.uix.dropdown
Principal:
    SeConnecter:
    CreerCompte:
    Accueil:

<SeConnecter>:
    name:"connect"
    BoxLayout:
        id:scnt
        orientation: "vertical"
        space_x: self.size[0]/3
        canvas.before:
            Color:
                rgba: C('#FFFFFF')
            Rectangle:
                size: self.size
                pos: self.pos
        BoxLayout:
            size_hint_y: None
            height:50
            padding:10
            canvas.before:
                Color:
                    rgba: C('#1B3E97')
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "Parc Auto"
                font_size:'25sp'
                bold: True
                size_hint_x: .0
        BoxLayout:
            size_hint_y: None
            height:50
            canvas.before:
                Color:
                    rgba: C('#163586')
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "AUTHENTIFICATION"
                font_size:'30sp'
                bold: True
                size_hint_x: .9

        BoxLayout:
            orientation:"vertical"
            padding: 300,200,300,100
            spacing: 20
            BoxLayout:
                orientation:"vertical"
                spacing: 10
                size_hint_y: None
                height: 80
                MDTextField:
                    id:username
                    hint_text: "Email"
                    multiline:False
                    mode: "fill"
                    icon_right:"email"
                    on_text_validate: pwd.focus = True
                MDTextField:
                    id:pwd
                    hint_text: "Mot de passe"
                    mode: "fill"
                    multiline:False
                    password: True
                    on_text_validate: root.validate_user()
                RelativeLayout:
                    size_hint : (0.5, 1.0)
                    MDIconButton:
                        icon: "eye-off"
                        ripple_scale: .5
                        pos: pwd.width - self.width + dp(8), 10
                        on_release:
                            self.icon = "eye" if self.icon == "eye-off" else "eye-off"
                            pwd.password = False if pwd.password is True else True
            Label:
                id: sp
                size_hint_y: None
                height: 40
            BoxLayout:
                orientation:'horizontal'
                padding:20
                Label:
                    size_hint_x:.7
                MDRoundFlatButton:
                    text:"Se connecter"
                    bold:True
                    text_color:C("#FFFFFF")
                    md_bg_color: "#1B3E97"
                    line_color: "#1B3E97"
                    size_hint_y: None
                    height: 30
                    size_hint_x:.99
                    on_release:root.validate_user()
            Label:
                id: sp2
            BoxLayout:
                orientation:'horizontal'
                size_hint_y: None
                height: 50
                spacing: 80
                Label:
                    text:'Pas de compte ?'
                    color:C('#000000')
                    bold: True
                    size_hint_x: .6
                Button:
                    background_color:(0,0,0,0)
                    text:"Créer un compte"
                    color:C('#000000')
                    bold:True
                    size_hint_y: None
                    height:50
                    on_release:app.root.current = 'compte'
        BoxLayout:
            size_hint_y: None
            height:20
            canvas.before:
                Color:
                    rgba: C('#163586')
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "Copyrigth@ tout droit reservé"
                bold: True
                size_hint_x: .9


<SmoothButton@Button>:
    background_color:(0,0,0,0)
    background_normal:''
    border_radius: [10]
    canvas.before:
        Color:
            rgba: C('#1B3E97')
        RoundedRectangle:
            size:self.size
            pos:self.pos
            #radius:self.border_radius


<CreerCompte>:
    name:"compte"
    BoxLayout:
        id:cmt
        orientation: "vertical"
        canvas.before:
            Color:
                rgba: C('#FFFFFF')
            Rectangle:
                size: self.size
                pos: self.pos
        BoxLayout:
            size_hint_y: None
            height:50
            padding:10
            canvas.before:
                Color:
                    rgba: C('#1B3E97')
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "Parc Auto"
                font_size:'25sp'
                bold: True
                size_hint_x: .0
        BoxLayout:
            size_hint_y: None
            height:50
            canvas.before:
                Color:
                    rgba: C('#163586')
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "COMPTE"
                font_size:'30sp'
                bold: True
                size_hint_x: .9
        FloatLayout:
            orientation:'vertical'
            size_hint_y:.3
            BoxLayout:
                orientation:"horizontal"
                spacing: 30
                padding:50,100,100,160
                BoxLayout:
                    orientation:"vertical"
                    spacing: 10
                    padding:10
                    size_hint_y: None
                    height: 80
                    Label:
                        text: "PRENOM"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:prenom
                        hint_text: "Prenom"
                        multiline:False
                        mode: "round"
                        icon_right:"account"
                    Label:
                        text: "NOM"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:nom
                        hint_text: "Nom"
                        icon_right:"account"
                        mode: "round"
                        multiline:False

                    Label:
                        text: "DATE NAISSANCE"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:age
                        hint_text: "AAAA-MM-JJ"
                        mode: "round"
                        multiline:False
                    RelativeLayout:
                        size_hint : (0.5, 1.0)
                        MDIconButton:
                            icon: "calendar"
                            ripple_scale: .5
                            pos: age.width - self.width + dp(3), 4
                            on_press:root.afficheDateN()

                    Label:
                        text: "SEXE"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:sexe
                        hint_text: "Selectionner"
                        icon_right:"human"
                        mode: "round"
                        multiline:False
                        on_focus: if self.focus: root.liste()
                BoxLayout:
                    orientation:"vertical"
                    spacing: 10
                    padding:10
                    size_hint_y: None
                    height: 80
                    Label:
                        text: "EMAIL"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:email
                        hint_text: "Email"
                        multiline:False
                        mode: "round"
                        icon_right:"email"
                    Label:
                        text: "FONCTION"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:fonc
                        hint_text: "Selectionner"
                        mode: "round"
                        multiline:False
                        on_focus: if self.focus: root.poste()

                    Label:
                        text: "DATE"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:dateIns
                        hint_text: "AAAA-MM-JJ"
                        mode: "round"
                        multiline:False
                    RelativeLayout:
                        size_hint : (0.5, 1.0)
                        MDIconButton:
                            icon: "calendar"
                            ripple_scale: .5
                            pos: txtPass.width - self.width + dp(0), 4
                            on_press:root.afficheDate()
                    Label:
                        text: "TELEPHONE"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:tel
                        hint_text: "Téléphone"
                        multiline:False
                        mode: "round"
                        icon_right:"phone"
                BoxLayout:
                    orientation:"vertical"
                    spacing: 10
                    padding:10,10,0,0
                    size_hint_y: None
                    height: 80
                    Label:
                        text: "NATIONALITE"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:nation
                        hint_text: "nationalité"
                        mode: "round"
                        multiline:False
                    Label:
                        text: "ADRESSE"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:adresse
                        hint_text: "Adresse"
                        icon_right:"map-marker"
                        mode: "round"
                        multiline:False
                    Label:
                        text: "MOT DE PASSE"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:txtPass
                        hint_text: "Mot de passe"
                        icon_left:"key-variant"
                        icon_right:
                        mode: "round"
                        password:True
                        multiline:False
                    RelativeLayout:
                        size_hint : (0.5, 1.0)
                        MDIconButton:
                            icon: "eye-off"
                            ripple_scale: .5
                            pos: txtPass.width - self.width + dp(8), 4
                            on_release:
                                self.icon = "eye" if self.icon == "eye-off" else "eye-off"
                                txtPass.password = False if txtPass.password is True else True
                    Label:
                        text: "CONFIRMER"
                        bold: True
                        color:C('#000000')
                    MDTextField:
                        id:confirm
                        hint_text: "confirmer"
                        icon_left:'key-variant'
                        mode: "round"
                        password:True
                        multiline:False
                    RelativeLayout:
                        size_hint : (0.5, 1.0)
                        MDIconButton:
                            icon: "eye-off"
                            ripple_scale: .5
                            pos: confirm.width - self.width + dp(8), 4
                            on_release:
                                self.icon = "eye" if self.icon == "eye-off" else "eye-off"
                                confirm.password = False if confirm.password is True else True
            BoxLayout:
                orientation:'horizontal'
                padding:200,100,200,50
                spacing:90
                AnnulerButton:
                    id:btnAnnuler
                    text:"Annuler"
                    bold:True
                    size_hint:(.3,.1)
                    on_release:app.root.current = 'connect'
                SmoothValidButton:
                    id:btnValider
                    text:"Valider"
                    bold:True
                    size_hint:(.3,.1)
                    on_release:root.validate_user()

        BoxLayout:
            size_hint_y: None
            height:20
            canvas.before:
                Color:
                    rgba: C('#163586')
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "Copyrigth@ tout droit reservé"
                bold: True
                size_hint_x: .9

<SmoothValidButton@Button>:
