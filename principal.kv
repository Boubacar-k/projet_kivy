    border_radius: [10]
    canvas.before:
        RoundedRectangle:
            pos:self.pos
            size:self.size
            #radius:self.border_radius
<SmoothButton@Button>:
    background_color:(0,0,0,0)
    background_normal:''
    border_radius: [10]
    canvas.before:
        Color:
            rgba: C('#FFFFFF')
        RoundedRectangle:
            size:self.size
            pos:self.pos
            #radius:self.border_radius

<deconnect@Button>:
